{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Hangfire": "Warning"
    }
  },
  "ConnectionStrings": {
    "ITaxDBConnection": "Server=localhost;Database=eTimSuite;UID=sageadmin;Password=sageadmin;MultipleActiveResultSets=True;Encrypt=False;App=iTaxMidware",
    "CacheConnection": "localhost,defaultDatabase=6,password=redispass"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Hangfire": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "../logs/itaxmidware_.log",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level:u3}] {Message:lj}{NewLine}{Exception}",
          "rollingInterval": "Day",
          "preserveLogFilename": true,
          "buffered": false
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "Properties": {
      "ApplicationName": "itaxmidware",
      "Environment": "Development"
    }
  },
  "AllowedHosts": "*"
}
